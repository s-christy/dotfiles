
*foreground: #AAAAAA
*foreground_bold: #a8a19f
*cursor: #a8a19f
#*background:       #1b1918
*background:       #1b1918

URxvt.intensityStyles:	false
URxvt.background:       [80]#000000
URxvt.depth:		32
URxvt.font: 		xft:monospace:size=9
URxvt.scrollBar:	false
URxvt.cursorColor:	white
#URxvt.cursorColor:	magenta

URxvt.perl-ext-common: default,resize-font,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up

URxvt.colorUL:		#4682B4

! in the future I want to change entire themes preferably by cycling with alt-tab, but if necessary by using C-#

! change to red background
URxvt.keysym.C-7: command:\033]11;#ff0000\007\033]10;yellow\007

! change to light background
URxvt.keysym.C-8: command:\033]11;#ffffff\007

! change to dark gray background
URxvt.keysym.C-9: command:\033]11;#777777\007

